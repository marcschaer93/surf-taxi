{
  "$schema": "https://json-schema.org/draft-07/schema",
  "$id": "http://example.com/example.json",
  "title": "Root Schema",
  "type": "object",
  "required": ["start_point", "end_point", "trip_info", "seats"],
  "properties": {
    "start_point": {
      "title": "The start_point Schema",
      "type": "string",
      "examples": ["Interlaken"]
    },
    "end_point": {
      "title": "The end_point Schema",
      "type": "string",
      "examples": ["Sidi Ifni"]
    },
    "trip_info": {
      "title": "The trip_info Schema",
      "type": "string",
      "examples": ["holiday"]
    },
    "seats": {
      "title": "The seats Schema",
      "type": "integer",
      "examples": [3]
    }
  },
  "examples": [
    {
      "start_point": "Interlaken",
      "end_point": "Sidi Ifni",
      "trip_info": "holiday",
      "seats": 3
    }
  ]
}
